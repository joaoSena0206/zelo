{
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://*:80"
            },
            "Https": {
                "Url": "https://*:443",
                "Certificate": {
                    "Path": "/etc/letsencrypt/live/zelo-server.com.br/fullchain.pem",
                    "KeyPath": "/etc/letsencrypt/live/zelo-server.com.br/privkey.pem"
                }
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}